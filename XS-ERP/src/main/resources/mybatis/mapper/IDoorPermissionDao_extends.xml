<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.xs.erp.dao.IDoorPermissionDao">

	<select id="selectByEmpID" resultMap="BaseResultMap" parameterType="Integer">
		SELECT id,emp_id,door_id,create_by,create_time FROM t_door_permission WHERE emp_id=#{id}
	</select>
</mapper>