package cn.productivetech.shtelcom.enrol.uarest.payload;

import java.util.ArrayList;
import java.util.List;

public class ProcessingInformation {

	private Biometric				biometric;
	private AudioCharacteristics	audioCharacteristics;
	private List<MetaInformation>	metaInformation;

	public ProcessingInformation() {
		this.biometric = new Biometric();
		this.audioCharacteristics = new AudioCharacteristics();
		this.metaInformation = new ArrayList<MetaInformation>();
		this.metaInformation.add(new MetaInformation());
	}

	public Biometric getBiometric() {
		return biometric;
	}

	public void setBiometri(Biometric biometric) {
		this.biometric = biometric;
	}

	public AudioCharacteristics getAudioCharacteristics() {
		return audioCharacteristics;
	}

	public void setAudioCharacteristics(AudioCharacteristics audioCharacteristics) {
		this.audioCharacteristics = audioCharacteristics;
	}

	public List<MetaInformation> getMetaInformation() {
		return metaInformation;
	}

	public void setMetaInformation(List<MetaInformation> metaInformation) {
		this.metaInformation = metaInformation;
	}

}
